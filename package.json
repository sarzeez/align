  "name": "align",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios-device": "react-native run-ios --device=\"iPhone (Александр)\"",
    "se": "react-native run-ios --simulator='iPhone SE (2nd generation)'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "podm1": "cd ios && arch -x86_64 pod install",
    "podrepom1": "cd ios && arch -x86_64 pod install --repo-update",
    "podupdatem1": "cd ios && arch -x86_64 pod update",
    "poddeintegratem1": "cd ios && arch -x86_64 pod deintegrate",
    "AAB": "cd android && ./gradlew bundleRelease",
    "APK": "cd android && ./gradlew assembleRelease",
    "postinstall": "patch-package",
    "reinstallModules": "watchman watch-del-all && rm -rf node_modules/ && rm -rf /tmp/metro-bundler-cache-* && rm -rf /tmp/haste-map-react-native-packager-* && yarn && cd ios && rm -rf Pods && pod install && cd ../",
    "connect": "adb reverse tcp:8081 tcp:8081"
  },
