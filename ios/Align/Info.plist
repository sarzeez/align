	<dict>
		<key>NSExceptionDomains</key>
		<dict>
			<key>localhost</key>
			<dict>
				<key>NSExceptionAllowsInsecureHTTPLoads</key>
				<true/>
			</dict>
		</dict>
	</dict>
	<key>NSAppleMusicUsageDescription</key>
	<string>Allow Align360 access to your play list</string>
	<key>NSCameraUsageDescription</key>
	<string>Allow Align360 access to your camera</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Allow Align360 access to your location while in use</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>Allow Align360 access to your photos library in order to upload photos</string>
